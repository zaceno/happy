{
    "name": "happy",
    "version": "2.0.0",
    "private": true,
    "description": "\"A web app for anonymously collecting the average happiness (on a scale 1-5) of a team\"",
    "main": "dist/index.html",
    "dependencies": {
        "hyperapp": "^2.0.3",
        "hyperapp-map": "^1.1.0",
        "poll": "^1.0.1"
    },
    "devDependencies": {
        "parcel-bundler": "^1.12.4"
    },
    "scripts": {
