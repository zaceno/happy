{
  "name": "zx-happy",
  "version": "1.0.0",
  "private": true,
  "description": "\"A web app for anonymously collecting the average happiness (on a scale 1-5) of a team\"",
  "main": "index.js",
  "dependencies": {
    "hyperx": "^2.3.0",
    "zx-app-utils": "^0.0.3"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "hyperxify": "^2.0.1",
    "less": "^2.7.2",
    "less-plugin-clean-css": "^1.5.1",
    "uglify-es": "^3.0.18",
    "uglifyify": "^4.0.1"
  },
  "scripts": {
    "build:js": "browserify src/index.js -t hyperxify | uglifyjs -c -m > dist/script.js",
    "build:html": "cp src/index.html dist/index.html",
    "build:css": "lessc --clean-css src/style/main.less > dist/style.css",
    "build": "npm run build:js && npm run build:html && npm run build:css"
  },
  "author": "Zacharias Enochsson",
  "license": "MIT"
}
